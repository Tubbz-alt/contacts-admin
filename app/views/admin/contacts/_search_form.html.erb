<%= simple_form_for search, url: admin_contacts_path, method: :get, html: { class: "form-search" } do |f| %>
  <%= f.input_field :title, placeholder: "Title", class: "search-query" %>
  <%= f.input_field :description, placeholder: "Description", class: "search-query" %>
  <%= f.submit "Filter", class: "btn btn-default", data: { disable_with: "Working" } %>
<% end %>
