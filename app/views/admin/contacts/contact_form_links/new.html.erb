<% content_for :page_title, 'Add contact form link' %>
<ol class="breadcrumb">
  <li><%= link_to 'Contacts', admin_contacts_path  %></li>
  <li><%= link_to @contact, edit_admin_contact_path(@contact) %></li>
  <li><%= link_to 'Online links', admin_contact_contact_form_links_path(@contact) %></li>
  <li class="active">Add link</li>
</ol>

<div class="page-title">
  <h1>
    <small><%= @contact %></small><br />
    Add contact form link
  </h1>
</div>

<%= render partial: 'form', locals: { contact: @contact, contact_form_link: @contact_form_link } %>
