<!DOCTYPE html>
<html>
<head>
  <title>HmrcContacts</title>
  <%= stylesheet_link_tag    "frontend", media: "all" %>
  <%= csrf_meta_tags %>
</head>
<body class="hmrc-contacts-body">
<% unless local_assigns[:show_breadcrumbs] %><div id="global-breadcrumb" class="header-context group"><ol><!-- deliberately empty --></ol></div><% end %>
<div id="wrapper">
  <div class="hmrc-contacts-content">
    <div id="page" class="<%= content_for?(:page_class) ? yield(:page_class) : '' %>" >
      <%= yield %>
    </div>
  </div>
</div>

<%= javascript_include_tag "frontend" %>
</body>
</html>
